{
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "alwaysOnTop": false,
        "closable": true,
        "decorations": false, 
        "fullscreen": false,
        "height": 600,
        "label": "main",
        "maximizable": true,
        "minHeight": 600,
        "minWidth": 1000,
        "minimizable": true,
        "resizable": true,
        "shadow": true,
        "theme": "Dark",
        "title": "Magic Eye",
        "titleBarStyle": "Visible",
        "url": "index.html",
       "userAgent": "Chrome",
        "visible": false,
        "width": 1000
      },
      {
        "alwaysOnTop": false,
        "decorations": false,
        "fullscreen": false,
        "label": "splashscreen",
        "minHeight": 600,
        "minWidth": 1000,
        "resizable": true,
        "title": "Magic Eye",
        "titleBarStyle": "Visible",
        "url": "splashscreen.html"
      }
    ],
    "withGlobalTauri": true
  },
  "build": {
    "beforeBuildCommand": "yarn build",
    "beforeDevCommand": "yarn dev",
    "devUrl": "http://127.0.0.1:1620/",
    "frontendDist": "../dist"
  },
  "bundle": {
    "active": true,
    "copyright": "MIT",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": [
      "./assets"
    ],
    "targets": "all",
    "windows": {
      "allowDowngrades": true,
      "certificateThumbprint": null,
      "digestAlgorithm": null,
      "timestampUrl": null,
      "tsp": false,
      "webviewFixedRuntimePath": null,
      "webviewInstallMode": {
        "silent": true,
        "type": "downloadBootstrapper"
      },
      "wix": null
    }
  },
  "identifier": "dev.MagicEye.magic",
  "plugins": {
    "allowlist": {
      "all": true,
      "window": {
        "center": false,
        "close": true,
        "create": false,
        "hide": false,
        "maximize": true,
        "minimize": true,
        "print": false,
        "requestUserAttention": false,
        "setAlwaysOnTop": false,
        "setCursorGrab": true,
        "setCursorIcon": true,
        "setCursorPosition": false,
        "setCursorVisible": false,
        "setDecorations": true,
        "setFocus": true,
        "setFullscreen": true,
        "setIcon": true,
        "setMaxSize": false,
        "setMinSize": false,
        "setPosition": false,
        "setResizable": true,
        "setSize": true,
        "setSkipTaskbar": false,
        "setTitle": true,
        "show": true,
        "startDragging": true,
        "unmaximize": true,
        "unminimize": true
      }
    },
    "fs": {
      "all": true,
      "scope": [
        "$RESOURCE"
      ]
    },
    "http": {
      "all": true,
      "request": true,
      "scope": [
        "http://*",
        "https://*"
      ]
    }
  },
  "productName": "Magic Eye",
  "version": "0.9.3"
}